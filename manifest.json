{
  "name": "Soundcloud StreamLine",
  "version": "1.0.0",
  "manifest_version": 2,

  "description": "Deletes previous songs on the Soundcloud stream to quicken it when a large amount of songs is loaded.",


  "author": "Andrew Lisowski",
  "homepage_url": "https://github.com/hipstersmoothie/soundcloud-optimize",
  "permissions": [
    "tabs",
    "activeTab",
    "https://soundcloud.com/stream"
  ],
  "content_scripts": [
    {
      "matches": [
        
        "https://soundcloud.com/stream"
      ],
      "js": [
        "jquery-3.1.0.slim.min.js",
        "removePrevious.js"
      ]
    }
  ],

  "browser_action": {
   "default_popup": "popup.html"
  }
}
